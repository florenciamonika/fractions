{
  "name": "fractions",
  "homepage": "http://build.com/fractions",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "build-css": "sass src/assets/scss:src/assets/css",
    "watch-css": "npm run build-css && sass --watch src/assets/scss:src/assets/css",
    "start": "npm-run-all -p watch-css react:start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
